{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": [
            "off",
            "double"
        ],
        "curly": "off",
        "linebreak-style": "off", 
        "no-alert": "off", 
        "consistent-return": "off", 
        "no-param-reassign": "warn"
    }
}
